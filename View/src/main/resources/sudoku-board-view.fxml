            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         </rowConstraints>
         <VBox.margin>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </VBox.margin>
      </GridPane>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button fx:id="backButton" onAction="#pressBack" prefHeight="78.0" prefWidth="504.0" styleClass="button-exit" text="%back.menu">
               <font>
                  <Font size="36.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" onAction="#onActionButtonCheck" prefHeight="78.0" prefWidth="325.0" text="%check.game">
               <font>
                  <Font size="36.0" />
               </font>
               <HBox.margin>
                  <Insets left="20.0" />
               </HBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#saveGameAction" prefHeight="78.0" prefWidth="216.0" text="%save.game">
               <font>
                  <Font size="36.0" />
               </font>
               <HBox.margin>
                  <Insets left="20.0" />
               </HBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#loadGameAction" prefHeight="78.0" prefWidth="249.0" text="%load.game">
               <font>
                  <Font size="36.0" />
               </font>
               <HBox.margin>
                  <Insets left="20.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
